    "deps:audit-fix": "npm audit fix --workspaces || node -e \"process.exit(0)\""
  },
  "dependencies": {},
  "devDependencies": {
    "next": "14.2.31",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.31",
    "postcss": "^8.4.35",
    "postcss-nested": "^6.2.0",
    "autoprefixer": "^10.4.16"
  },
  "engines": {
    "node": "20.x"
  },
  "packageManager": "npm@10.7.0"
}
