{
  "name": "istampit-web",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "scripts": {
  "prebuild": "node scripts/prebuild.mjs",
    "build": "next build",
    "export": "echo 'use build only; output:export handles static generation'",
    "build:static": "npm run build && npm run export",
    "vercel-build": "next build",
    "dev": "next dev",
    "lint": "next lint",
    "validate-env": "echo 'skip env validation in CI'",
    "test": "echo 'No tests configured'",
    "test:unit": "vitest run",
    "test:e2e": "playwright test"
  },
  "dependencies": {
    "next": "15.4.6",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "tailwindcss": "3.4.17"
  },
  "devDependencies": {
    "@playwright/test": "^1.47.2",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.2",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.4.6",
    "postcss": "^8.4.40",
    "typescript": "^5.5.4",
    "vitest": "^1.6.0"
  },
  "overrides": {
    "esbuild": "^0.25.0",
    "tmp": "^0.2.3"
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "not op_mini all"
  ]
