    "test:e2e": "playwright test"
  },
  "dependencies": {
    "ioredis": "^5.4.1",
    "next": "15.4.7",
    "react": "19.1.1",
    "react-dom": "19.1.1",
  "tailwindcss": "4.1.13"
  },
  "devDependencies": {
    "@playwright/test": "^1.47.2",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.7",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.33.0",
    "eslint-config-next": "15.5.0",
    "postcss": "^8.4.40",
    "typescript": "^5.5.4",
    "vitest": "^3.2.4"
  },
  "overrides": {
    "esbuild": "^0.25.0",
    "tmp": "^0.2.3"
