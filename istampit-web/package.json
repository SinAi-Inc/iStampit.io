  "private": true,
  "license": "MIT",
  "type": "module",
  "scripts": {
    "prebuild": "node scripts/prebuild.mjs",
    "build": "next build",
  "start": "next start",
    "export": "echo 'use build only; output:export handles static generation'",
    "build:static": "npm run build && npm run export",
    "vercel-build": "next build",
    "dev": "next dev",
